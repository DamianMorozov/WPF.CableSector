<Page x:Class="CableSector.Views.PageChangeLog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d" 
      d:DesignWidth="970" d:DesignHeight="760"
      Title="Предварительный просмотр" Loaded="PageChangeLog_OnLoaded"
      Background="Transparent" FontSize="18">

    <Grid Name="GridMain">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <ScrollViewer>
            <TextBlock Name="TextBlockMain" Margin="5" Foreground="Black" FontSize="15" ></TextBlock>
        </ScrollViewer>
    </Grid>
</Page>
